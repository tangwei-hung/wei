def find_pairs():
    # The input format of the list of integers is like 1 2 3 4 5 6 
    user_number_list=list(map(int,input('Please enter the numbers:').split()))
    pairs_number=[]
    for i in user_number_list:
        for j in user_number_list[i:]:
            if (i*j % 2) == 0 and ((i+j) % 2)==1:
                pairs_number.append((i,j))
    return pairs_number


if __name__ == '__main__':
    print(find_pairs())
